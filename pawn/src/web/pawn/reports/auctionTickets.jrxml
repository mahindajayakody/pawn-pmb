	<queryString>
		<![CDATA[SELECT t.tktno,
     t.`condate`,
     t.`pawnadv`,
     t.`tktno`,
     t.`MKTVALUE` AS mktvalue,
     t.`NETWEIGHT` AS netweight,
     d.`BALAMOUNT` AS tblduefrom_BALAMOUNT,
     p.`NAME` AS tblpawner_NAME,
     p.`INITIALS` AS tblpawner_INITIALS,
     p.`CLTITLE` AS tblpawner_CLTITLE,
     p.IDNO as "NIC",
     p.hometpno as "home",
     p.mobileno as "mobile",
/*     a.description as "desc",*/
     d.`DUEAMOUNT` AS "DUEAMOUNT",
     at.AUCTIONEXP as "cost",
     at.UPSETVALUE as "UPSETVALUE",
     at.SOLDAMT as "SOLDAMT",
     at.EXCESS as "EXCESS",
     t.GROSSWEIGHT as "GROSSWEIGHT",
(select compname from tblusercompany ) as comp
FROM tblauctionticket at
INNER JOIN tblticket t ON at.tktid = t.TICKETID
/*INNER JOIN tblticketarticle ta ON t.`TICKETID` = ta.`TICKETID`*/
INNER JOIN tblduefrom d ON t.`TICKETID` = d.`TICKETID`
INNER JOIN tblpawner p ON t.`PWNID` = p.`PWNID`
/*INNER JOIN tblarticle a ON ta.ARTICALID = a.ARTICALID*/
INNER JOIN tblauctionmaster am on am.auctionid = at.AUCTIONID
WHERE d.`duetypeid` = 2
and ($P{jpIntBranchId} = 999 or at.branchid = $P{jpIntBranchId})
and am.auctiondate >=  $P{jpFromDate}
and am.auctiondate <= $P{jpToDate}]]>
	</queryString>
