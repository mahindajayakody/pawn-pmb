	<queryString>
		<![CDATA[SELECT

     ( SELECT IFNULL(SUM(due.DUEAMOUNT),0)
	FROM  tblduefrom due
	WHERE due.`BRANCHID` = br.`BRANCHID`
		AND due.`DUETYPEID` = 2
		AND  DATE(due.`LASTUPDATE`) >=  $P{jpFromDate}
		AND  DATE(due.`LASTUPDATE`) <=  $P{jpToDate}) as int_due,


      ( SELECT IFNULL(SUM(dr.`SETAMOUNT`),0)
	FROM `tblduereceipt` dr INNER JOIN `tblticket` ti ON dr.`TICKETID` = ti.`TICKETID`
	WHERE dr.`DUETYPEID` = 2
		AND dr.`BRANCHID` = br.`BRANCHID`
 		AND DATE(dr.`SETDATE`) >= $P{jpFromDate}
 		AND DATE(dr.`SETDATE`) <= $P{jpToDate}
		AND ti.`STSID` = 5) as redeem_col,

      ( SELECT IFNULL(SUM(dr.`SETAMOUNT`),0)
	FROM `tblduereceipt` dr INNER JOIN `tblticket` ti ON dr.`TICKETID` = ti.`TICKETID`
	WHERE dr.`DUETYPEID` = 2
		AND dr.`BRANCHID` = br.`BRANCHID`
 		AND DATE(dr.`SETDATE`) >= $P{jpFromDate}
 		AND DATE(dr.`SETDATE`) <= $P{jpToDate}
		AND ti.`STSID` <> 5) as part_set,

     br.`BRANCHID` AS tblbranch_BRANCHID,
     br.`BRANCHNAME` AS tblbranch_BRANCHNAME,
     (select compname from tblusercompany ) as comp
FROM `tblbranch` br
WHERE ($P{jpIntBranchId} = 999 or br.`branchid` = $P{jpIntBranchId}) AND br.code <> "***"]]>
	</queryString>
